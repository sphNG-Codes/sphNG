      REAL radius, length, Muff, ampl, vampl
      PARAMETER(radius = 1.d0)
      PARAMETER(length = 1.d0)
      PARAMETER(Muff   = 2.5d0)
      PARAMETER(ampl   = 0.001d0)
      PARAMETER(vampl  = 0.1)  !absolute v amplitude is ampl*vampl
      
