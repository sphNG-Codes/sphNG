      INTEGER nbinmax
      PARAMETER (nbinmax = 30)

      INTEGER nlstbins, it1bin, it2bin, it2, listbins, nparentibin
      INTEGER itbinupdate,ibincurrent,ibinlast

      COMMON /timeextra/ nlstbins(nbinmax),it1bin(nbinmax),
     &      it2bin(nbinmax),it2(idim),listbins(idim,nbinmax),
     &      nparentibin(nbinmax),itbinupdate,ibincurrent,ibinlast

